{
    "name": "Project_x",
    "description": "search for keywords",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "app": {
        "launch": {
            "web_url": "http://mysubdomain.example.com/"
        }
    },
    "background": {
        "scripts": ["index.js"]
      },
      "page_action": {
        "default_popup": "index.html"
      },
    "permissions": [
        "<all_urls>"

    ]
}